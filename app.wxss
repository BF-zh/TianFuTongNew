.triangle{
  --c:var(--secondary-color);
  --size:.5em;
  display: inline-block;
  width: var(--size);
  height: var(--size);
  border-left: var(--size) solid transparent;
  border-bottom: var(--size) solid var(--c);
  transform: rotate(45deg);
}
